  },
  {
    "from": "dateIntervalInput-travelPeriod-action2",
    "to": "checkOut"
  },
  {
    "from": "",
    "to": "lang",
    "default":"ru_RU"
  },
  {
    "from": "",
    "to": "currency",
    "default":"RUB"
  },
  {
    "from": "textinput-numberOfPersons-action",
    "to": "adults"
  }
],
  "source": "hotellook-find-hotels"
}